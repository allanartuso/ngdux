  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "nx run-many --all --target=test",
    "lint": "nx run-many --all --target=lint",
    "e2e": "ng e2e",
    "e2e:demo:local": "node ./tools/e2e/cy-integration-local.js && npx cypress run -P ./apps/demo-e2e --headless --config-file=cypress-integration-local.config.ts",
    "e2e:demo:watch": "npx cypress open -P ./apps/demo-e2e",
    "test:ci": " npx jest --ci",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ngdux:build": "nx build ngdux-util-store-form --configuration=production && nx build ngdux-util-store-list --configuration=production",
    "ngdux:publish": "npm publish dist/libs/ngdux/data-model/common && npm publish dist/libs/ngdux/util/store/common && npm publish dist/libs/ngdux/util/store/form && npm publish dist/libs/ngdux/util/store/list",
    "ngil:build": "nx build ngil-ui-common-form-cva --configuration=production && nx build ngil-ui-list --configuration=production",
    "ngil:publish": "npm publish dist/libs/ngil/ui/common/form-cva && npm publish dist/libs/ngil/ui/list",
    "update-versions": "ts-node -P tools/tsconfig.json tools/versioning/update-versions.ts",
    "publish": "ts-node -P tools/tsconfig.json tools/versioning/publish-libs.ts",
    "generate:db": "ts-node -P tools/tsconfig.json tools/generators/db-json.ts"
  },

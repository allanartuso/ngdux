    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/demo/feature/users/src/**/*.ts", "libs/demo/feature/users/src/**/*.html"]
      },
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": ["scope:demo", "type:feature"]
}

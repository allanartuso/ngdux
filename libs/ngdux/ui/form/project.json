{
  "name": "ngdux-ui-form",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ngdux/ui/form/src",
  "prefix": "demo",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/ngdux/ui/form"],
      "options": {
        "project": "libs/ngdux/ui/form/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ngdux/ui/form/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ngdux/ui/form/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ngdux/ui/form"],
      "options": {
        "jestConfig": "libs/ngdux/ui/form/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/ngdux/ui/form/**/*.ts", "libs/ngdux/ui/form/**/*.html"]
      }
    }
  },
  "tags": []
}
